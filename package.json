{
  "name": "plugnmeet-client",
  "version": "1.2.3",
  "author": "Jibon L. Costa",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build-netlify": "npm run build && cp dist/assets/config_sample.js dist/assets/config.js",
    "lint": "prettier --write \"./src/**/*.{ts,tsx,scss}\" && eslint --fix --ext .tsx,.ts src/",
    "format-check": "prettier --check \"./src/**/*.{ts,tsx,scss}\" && eslint --quiet --ext .tsx,.ts src/",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "proto": "protoc --es_opt=target=js+dts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=./src/helpers/proto --proto_path=./protocol/proto_files plugnmeet_datamessage.proto plugnmeet_recorder.proto plugnmeet_recording.proto plugnmeet_breakout_room.proto plugnmeet_polls.proto plugnmeet_common_api.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "0.1.1",
    "@excalidraw/excalidraw": "0.12.0",
    "@headlessui/react": "1.7.1",
    "@reduxjs/toolkit": "1.8.5",
    "@tensorflow-models/body-pix": "2.2.0",
    "@tensorflow/tfjs": "3.20.0",
    "axios": "0.27.2",
    "copy-text-to-clipboard": "3.0.1",
    "emoji-picker-react": "3.6.2",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-http-backend": "1.4.1",
    "livekit-client": "1.3.3",
    "lodash": "4.17.21",
    "mobile-detect": "1.4.5",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "3.4.7",
    "react-i18next": "11.18.6",
    "react-player": "2.10.1",
    "react-redux": "8.0.2",
    "react-toastify": "9.0.8",
    "resumablejs": "1.1.0",
    "sanitize-html": "2.7.1",
    "validator": "13.7.0",
    "video.js": "7.20.2"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "0.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@types/lodash": "4.14.185",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/sanitize-html": "2.6.2",
    "@types/video.js": "7.3.47",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "10.4.10",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.1.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "postcss-nested": "5.0.6",
    "prettier": "2.7.1",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.7",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "tailwindcss": "3.1.8",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.3.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.0"
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "safari >= 11",
    "ios_saf >= 11",
    "chrome >= 64",
    "and_chr >= 64",
    "android >= 64",
    "firefox >= 53",
    "and_ff >= 53",
    "edge >= 79",
    "Opera >= 52",
    "Samsung >= 9.2",
    "not IE 11",
    "not dead"
  ]
}
