{
  "name": "plugnmeet-client",
  "version": "1.1.8",
  "author": "Jibon L. Costa",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build-netlify": "npm run build && cp dist/assets/config_sample.js dist/assets/config.js",
    "lint": "prettier --write \"./src/**/*.{ts,tsx,scss}\" && eslint --fix --ext .tsx,.ts src/",
    "format-check": "prettier --check \"./src/**/*.{ts,tsx,scss}\" && eslint --quiet --ext .tsx,.ts src/",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "proto": "protoc --es_opt=target=ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=./src/helpers/proto --proto_path=./protocol/proto_files ./protocol/proto_files/plugnmeet_datamessage.proto"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "0.12.0",
    "@headlessui/react": "1.6.6",
    "@reduxjs/toolkit": "1.8.3",
    "@tensorflow-models/body-pix": "2.2.0",
    "@tensorflow/tfjs": "3.19.0",
    "axios": "0.27.2",
    "copy-text-to-clipboard": "3.0.1",
    "i18next": "21.8.16",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.1",
    "livekit-client": "1.2.10",
    "lodash": "4.17.21",
    "mobile-detect": "1.4.5",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "3.4.7",
    "react-i18next": "11.18.3",
    "react-player": "2.10.1",
    "react-redux": "8.0.2",
    "react-toastify": "9.0.7",
    "resumablejs": "1.1.0",
    "sanitize-html": "2.7.1",
    "validator": "13.7.0",
    "video.js": "7.20.1"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "0.0.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@types/lodash": "4.14.182",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/sanitize-html": "2.6.2",
    "@types/video.js": "7.3.44",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "autoprefixer": "10.4.8",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "postcss-nested": "5.0.6",
    "prettier": "2.7.1",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.7",
    "sass": "1.54.3",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "tailwindcss": "3.1.8",
    "terser-webpack-plugin": "5.3.3",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
