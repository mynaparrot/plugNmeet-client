{
  "name": "plugnmeet-client",
  "version": "1.6.0",
  "author": "Jibon L. Costa",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build-netlify": "npm run build && cp dist/assets/config_sample.js dist/assets/config.js",
    "lint": "prettier --write \"./src/**/*.{ts,tsx,scss}\" && eslint --fix --ext .tsx,.ts src/",
    "format-check": "prettier --check \"./src/**/*.{ts,tsx,scss}\" && eslint --quiet --ext .tsx,.ts src/",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "size-limit": "npm run build && size-limit --json"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "@excalidraw/excalidraw": "0.17.6",
    "@headlessui/react": "1.7.19",
    "@nats-io/jetstream": "^3.0.0-4",
    "@nats-io/nats-core": "^3.0.0-17",
    "@reduxjs/toolkit": "2.2.7",
    "@tensorflow-models/body-pix": "2.2.1",
    "@tensorflow/tfjs": "4.20.0",
    "axios": "1.7.4",
    "copy-text-to-clipboard": "3.2.0",
    "emoji-picker-react": "4.11.1",
    "eventemitter3": "5.0.1",
    "i18next": "23.13.0",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-http-backend": "2.5.2",
    "livekit-client": "2.5.0",
    "lodash": "4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "1.40.0",
    "mobile-detect": "1.4.5",
    "nosleep.js": "0.12.0",
    "plugnmeet-protocol-js": "^1.0.3",
    "react": "18.3.1",
    "react-cool-virtual": "0.7.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.3.1",
    "react-draggable": "npm:@tdaniil/react-draggable@5.0.1",
    "react-hotkeys-hook": "4.5.0",
    "react-i18next": "15.0.1",
    "react-player": "2.16.0",
    "react-redux": "9.1.2",
    "react-toastify": "10.0.5",
    "resumablejs": "1.1.0",
    "sanitize-html": "2.13.0",
    "validator": "13.12.0",
    "video.js": "8.17.3"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@size-limit/preset-app": "11.1.4",
    "@types/emscripten": "1.39.13",
    "@types/lodash": "4.17.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/sanitize-html": "2.11.0",
    "@types/video.js": "7.3.58",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "autoprefixer": "10.4.19",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "mini-css-extract-plugin": "2.9.0",
    "postcss": "8.4.38",
    "postcss-loader": "8.1.1",
    "postcss-nested": "6.0.1",
    "prettier": "3.3.2",
    "react-refresh": "0.14.2",
    "react-refresh-typescript": "2.0.9",
    "sass": "1.77.5",
    "sass-loader": "14.2.1",
    "size-limit": "11.1.4",
    "style-loader": "4.0.0",
    "tailwindcss": "3.4.10",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "size-limit": [
    {
      "name": "JS files",
      "path": [
        "dist/assets/js/*.js",
        "dist/assets/lti/js"
      ]
    },
    {
      "name": "CSS files",
      "path": [
        "dist/assets/css",
        "dist/assets/lti/css"
      ]
    },
    {
      "name": "Locales files",
      "path": "dist/assets/locales"
    },
    {
      "name": "Image files",
      "path": [
        "dist/assets/imgs",
        "dist/assets/backgrounds"
      ]
    },
    {
      "name": "Font files",
      "path": "dist/assets/fonts"
    },
    {
      "name": "Webassembly files",
      "path": [
        "dist/assets/js/*.wasm",
        "dist/assets/models"
      ]
    },
    {
      "name": "All assets",
      "path": "dist/assets"
    }
  ],
  "browserslist": [
    "safari >= 11",
    "ios_saf >= 11",
    "chrome >= 64",
    "and_chr >= 64",
    "android >= 64",
    "firefox >= 53",
    "and_ff >= 53",
    "edge >= 79",
    "Opera >= 52",
    "Samsung >= 9.2",
    "not IE 11",
    "not dead"
  ]
}
