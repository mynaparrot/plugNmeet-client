{
  "name": "plugnmeet-client",
  "version": "1.5.4",
  "author": "Jibon L. Costa",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build-netlify": "npm run build && cp dist/assets/config_sample.js dist/assets/config.js",
    "lint": "prettier --write \"./src/**/*.{ts,tsx,scss}\" && eslint --fix --ext .tsx,.ts src/",
    "format-check": "prettier --check \"./src/**/*.{ts,tsx,scss}\" && eslint --quiet --ext .tsx,.ts src/",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "proto": "protoc --es_opt=target=ts,import_extension=.ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=./src/helpers/proto --proto_path=./protocol/proto_files plugnmeet_datamessage.proto plugnmeet_recorder.proto plugnmeet_recording.proto plugnmeet_breakout_room.proto plugnmeet_polls.proto plugnmeet_common_api.proto plugnmeet_ingress.proto plugnmeet_speech_services.proto plugnmeet_analytics.proto",
    "size-limit": "npm run build && size-limit --json"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.3.1",
    "@excalidraw/excalidraw": "0.16.1",
    "@headlessui/react": "1.7.17",
    "@reduxjs/toolkit": "1.9.6",
    "@tensorflow-models/body-pix": "2.2.1",
    "@tensorflow/tfjs": "4.11.0",
    "axios": "1.5.0",
    "copy-text-to-clipboard": "3.2.0",
    "emoji-picker-react": "4.5.2",
    "eventemitter3": "5.0.1",
    "i18next": "23.5.1",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.2",
    "livekit-client": "1.13.4",
    "lodash": "4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "1.32.0",
    "mobile-detect": "1.4.5",
    "nosleep.js": "0.12.0",
    "react": "18.2.0",
    "react-cool-virtual": "0.7.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-draggable": "npm:@tdaniil/react-draggable@5.0.1",
    "react-hotkeys-hook": "4.4.1",
    "react-i18next": "13.2.2",
    "react-player": "2.13.0",
    "react-redux": "8.1.2",
    "react-toastify": "9.1.3",
    "reconnecting-websocket": "4.4.0",
    "resumablejs": "1.1.0",
    "sanitize-html": "2.11.0",
    "validator": "13.11.0",
    "video.js": "8.5.2"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "1.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@size-limit/preset-app": "9.0.0",
    "@types/emscripten": "1.39.8",
    "@types/lodash": "4.14.199",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/sanitize-html": "2.9.0",
    "@types/video.js": "7.3.52",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "autoprefixer": "10.4.16",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.30",
    "postcss-loader": "7.3.3",
    "postcss-nested": "6.0.1",
    "prettier": "3.0.3",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.9",
    "sass": "1.68.0",
    "sass-loader": "13.3.2",
    "size-limit": "9.0.0",
    "style-loader": "3.3.3",
    "tailwindcss": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.4.4",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "size-limit": [
    {
      "name": "JS files",
      "path": [
        "dist/assets/js/*.js",
        "dist/assets/lti/js"
      ]
    },
    {
      "name": "CSS files",
      "path": [
        "dist/assets/css",
        "dist/assets/lti/css"
      ]
    },
    {
      "name": "Locales files",
      "path": "dist/assets/locales"
    },
    {
      "name": "Image files",
      "path": [
        "dist/assets/imgs",
        "dist/assets/backgrounds"
      ]
    },
    {
      "name": "Font files",
      "path": "dist/assets/fonts"
    },
    {
      "name": "Webassembly files",
      "path": [
        "dist/assets/js/*.wasm",
        "dist/assets/models"
      ]
    },
    {
      "name": "All assets",
      "path": "dist/assets"
    }
  ],
  "browserslist": [
    "safari >= 11",
    "ios_saf >= 11",
    "chrome >= 64",
    "and_chr >= 64",
    "android >= 64",
    "firefox >= 53",
    "and_ff >= 53",
    "edge >= 79",
    "Opera >= 52",
    "Samsung >= 9.2",
    "not IE 11",
    "not dead"
  ]
}
