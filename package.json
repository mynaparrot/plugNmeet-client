{
  "name": "plugnmeet-client",
  "version": "1.5.15",
  "author": "Jibon L. Costa",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build-netlify": "npm run build && cp dist/assets/config_sample.js dist/assets/config.js",
    "lint": "prettier --write \"./src/**/*.{ts,tsx,scss}\" && eslint --fix --ext .tsx,.ts src/",
    "format-check": "prettier --check \"./src/**/*.{ts,tsx,scss}\" && eslint --quiet --ext .tsx,.ts src/",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "proto": "protoc --es_opt=target=ts,import_extension=.ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=./src/helpers/proto --proto_path=./protocol/proto_files plugnmeet_datamessage.proto plugnmeet_recorder.proto plugnmeet_recording.proto plugnmeet_breakout_room.proto plugnmeet_polls.proto plugnmeet_common_api.proto plugnmeet_ingress.proto plugnmeet_speech_services.proto plugnmeet_analytics.proto",
    "size-limit": "npm run build && size-limit --json"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.7.2",
    "@excalidraw/excalidraw": "0.17.3",
    "@headlessui/react": "1.7.18",
    "@reduxjs/toolkit": "2.2.1",
    "@tensorflow-models/body-pix": "2.2.1",
    "@tensorflow/tfjs": "4.17.0",
    "axios": "1.6.7",
    "copy-text-to-clipboard": "3.2.0",
    "emoji-picker-react": "4.8.0",
    "eventemitter3": "5.0.1",
    "i18next": "23.10.1",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.5.0",
    "livekit-client": "2.0.7",
    "lodash": "4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "1.36.0",
    "mobile-detect": "1.4.5",
    "nosleep.js": "0.12.0",
    "react": "18.2.0",
    "react-cool-virtual": "0.7.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-draggable": "npm:@tdaniil/react-draggable@5.0.1",
    "react-hotkeys-hook": "4.5.0",
    "react-i18next": "14.1.0",
    "react-player": "2.15.1",
    "react-redux": "9.1.0",
    "react-toastify": "10.0.4",
    "reconnecting-websocket": "4.4.0",
    "resumablejs": "1.1.0",
    "sanitize-html": "2.12.1",
    "validator": "13.11.0",
    "video.js": "8.10.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "1.7.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@size-limit/preset-app": "11.1.0",
    "@types/emscripten": "1.39.10",
    "@types/lodash": "4.17.0",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.22",
    "@types/sanitize-html": "2.11.0",
    "@types/video.js": "7.3.57",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "autoprefixer": "10.4.18",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "6.10.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "mini-css-extract-plugin": "2.8.1",
    "postcss": "8.4.35",
    "postcss-loader": "8.1.1",
    "postcss-nested": "6.0.1",
    "prettier": "3.2.5",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.9",
    "sass": "1.72.0",
    "sass-loader": "14.1.1",
    "size-limit": "11.1.0",
    "style-loader": "3.3.4",
    "tailwindcss": "3.4.1",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "9.5.1",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "size-limit": [
    {
      "name": "JS files",
      "path": [
        "dist/assets/js/*.js",
        "dist/assets/lti/js"
      ]
    },
    {
      "name": "CSS files",
      "path": [
        "dist/assets/css",
        "dist/assets/lti/css"
      ]
    },
    {
      "name": "Locales files",
      "path": "dist/assets/locales"
    },
    {
      "name": "Image files",
      "path": [
        "dist/assets/imgs",
        "dist/assets/backgrounds"
      ]
    },
    {
      "name": "Font files",
      "path": "dist/assets/fonts"
    },
    {
      "name": "Webassembly files",
      "path": [
        "dist/assets/js/*.wasm",
        "dist/assets/models"
      ]
    },
    {
      "name": "All assets",
      "path": "dist/assets"
    }
  ],
  "browserslist": [
    "safari >= 11",
    "ios_saf >= 11",
    "chrome >= 64",
    "and_chr >= 64",
    "android >= 64",
    "firefox >= 53",
    "and_ff >= 53",
    "edge >= 79",
    "Opera >= 52",
    "Samsung >= 9.2",
    "not IE 11",
    "not dead"
  ]
}
